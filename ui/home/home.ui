QScrollBar:vertical {
    border: 2px solid #cccccc;  /* Viền ngoài màu xám */
    background: #f0f0f0;        /* Màu nền thanh cuộn */
    width: 16px;                /* Tăng chiều rộng */
    border-radius: 5px;         /* Bo tròn toàn bộ thanh */
}

QScrollBar::handle:vertical {
	background-color: rgb(56, 106, 236);
    border-radius: 4px !important; 
}

QScrollBar::handle:vertical:hover {
    background: #0056b3;        /* Màu đậm hơn khi hover */
}

QScrollBar::add-line:vertical, 
QScrollBar::sub-line:vertical {
    background: none;
    border: none;
    width: 0px;                 /* Ẩn các nút tăng giảm */
}

QScrollBar::add-page:vertical, 
QScrollBar::sub-page:vertical {
    background: none;
}</string>
